{
  "name": "magician",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/magician.mjs"
    },
    "./cjs": {
      "types": "./dist/index.d.mts",
      "require": "./dist/magician.cjs"
    },
    "./iife": {
      "browser": "./dist/magician.iife.js"
    }
